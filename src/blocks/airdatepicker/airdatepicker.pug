div.flex-wrap
    div.arrow__wrapper
        input(type='text'
            data-range='true'
            data-multiple-dates-separator=' - ' 
            class='dateRange range'
            class='datepicker-here'
            id='startDate'
            placeholder='ДД.ММ.ГГГГ')
            
    div.arrow__wrapper
        input(type='text'
            class='range'
            id='endDate'
            placeholder='ДД.ММ.ГГГГ')

